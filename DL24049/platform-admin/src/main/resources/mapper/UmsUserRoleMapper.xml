<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.platform.com.auth.mapper.UmsUserRoleMapper">

    <select id="countByRoleId" parameterType="long" resultType="int">
        select count(*) from ums_user_role where role_id = #{roleId}
    </select>

</mapper>
